<div class="row">
  <div class="col-sm-4">
    {{#link-to 'announcements'}}<h4>Announcements</h4>{{/link-to}}

    {{#link-to 'about'}}About{{/link-to}}<br>
    {{#link-to 'contact'}}Contact Us{{/link-to}}
  </div>
  <div class="col-sm-8">
    <ul>
      {{#each model.rentals as |rental|}}
        {{rental-tile rental=rental update="update" destroyRental="destroyRental"}}
      {{/each}}
    </ul>
  </div>
</div>
<div class="row">
  <div class="col-sm-4">
  </div>
  <div class="col-sm-8">
    <h2>Cities</h2>
    <ul>
      {{#each model.cities as |city|}}
        <li>{{city-popularity city}} {{#link-to 'city' city.id}}{{city.name}}{{/link-to}}</li>
      {{/each}}
    </ul>
  </div>
</div>
<div class="row">
  <div class="col-sm-4">

  </div>
  <div class="col-sm-8">
    {{new-city saveCity2="saveCity3"}}
  </div>
</div>
